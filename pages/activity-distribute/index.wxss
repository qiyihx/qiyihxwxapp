.container{
    width: 100%;
    background-color: #f0eff5;
}

.mine{
    margin-top: 20rpx;
    width: 100%;
    position: relative;
    background-color: #fff;
    border-style: solid;
    border-color: #f0eff5;
    border-width: 1rpx;
}

.line_cls{
    width: 100%; 
    height: 90rpx;
    line-height: 90rpx;
    position: relative;
}

.line{
    height: 2rpx;
    background-color: #f0eff5;
    position: relative;
    margin-left: 30rpx;
}

.line_head_cls{
    width: 100%; 
    height: 150rpx;
    line-height: 150rpx;
    position: relative;
}

.line_key_cls{
    position: absolute;
    left: 30rpx;
    font-weight:400;
}

.line_val_cls{
    position: absolute;
    right: 60rpx;
    color: #b4b4b4;
}

.line_val2_cls{
    position: absolute;
    right: 30rpx;
    color: #b4b4b4;
}

.line_val15_cls{
    position: absolute;
    right: 15rpx;
    color: #b4b4b4;
}

.right_icon_cls{ 
    position: absolute;
    right: 15rpx;
    top: 5rpx;
}

.right_icon2_cls{
    position: absolute;
    right: 0rpx;
    top: 5rpx
}

.picker{
    padding-right: 45rpx;
    color: #b4b4b4;
}

.right_icon_img{ 
    height: 30rpx;
    width: 30rpx;
}

.pic-cls{
   width: 100%;
   text-align: center;
   height: 446rpx;
   position: fixed;
   bottom: 0rpx;
}

.pic-img{
    width: 100%;
    height: 446rpx;
}
